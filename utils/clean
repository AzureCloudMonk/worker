#!/usr/bin/env bash

set -e
shopt -s nullglob

cd $(dirname $(dirname ${BASH_SOURCE[0]}))

rm -rvf \
  ./bin/travis-worker \
  ${GOPATH%%:*}/bin/travis-worker \
  coverage.html \
  *coverage.coverprofile \
  ./tmp/deb \
  ./tmp/output

find ${GOPATH%%:*}/pkg -wholename '*travis-ci/worker*.a' | xargs rm -vf
